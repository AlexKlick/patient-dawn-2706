<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @competition.name %>
</p>

<p>
  <strong>Location:</strong>
  <%= @competition.location %>
</p>

<p>
  <strong>Sport:</strong>
  <%= @competition.sport %>
</p>
<% @competition.teams.each do |team| %>
  <%= team.hometown %>
  <%= team.nickname %>
<% end %>

<%= link_to 'Edit', edit_competition_path(@competition) %> |
<%= link_to 'Back', competitions_path %>
